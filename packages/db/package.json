{
  "name": "@everyskill/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "echo 'No unit tests yet' && exit 0",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@everyskill/core": "workspace:*",
    "drizzle-orm": "^0.42.0",
    "postgres": "^3.4.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "next-auth": "5.0.0-beta.30",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
